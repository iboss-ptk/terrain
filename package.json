{
  "name": "@iboss/terrain",
  "description": "Terra development environment",
  "version": "0.0.9",
  "__comment": "^ update template package.json to the same version when publish",
  "author": "Supanat Potiwarakorn @iboss-ptk",
  "bin": {
    "terrain": "./bin/run"
  },
  "bugs": "https://github.com/terrain/issues",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@oclif/dev-cli": "^1.26.0",
    "@types/adm-zip": "^0.4.34",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^10.17.60",
    "@types/ramda": "^0.26.0",
    "@types/superagent": "^4.1.13",
    "babel-jest": "^27.3.1",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "jest": "^27.3.1",
    "prettier": "2.4.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.10"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/iboss-ptk/terrain",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "terrain",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "helpClass": "./src/lib/help"
  },
  "repository": "https://github.com/iboss-ptk/terrain",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "fixlint": "eslint . --ext .ts --config .eslintrc --fix",
    "prepack": "rm -rf lib && tsc -b && cp -R src/template lib/template && oclif-dev manifest && oclif-dev readme",
    "test": "jest",
    "test:watch": "jest --watch",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/core": "^1.0.11",
    "@oclif/plugin-help": "^3.2.3",
    "@terra-money/terra.js": "^3.1.3",
    "adm-zip": "^0.5.9",
    "cli-ux": "^5.6.3",
    "fs-extra": "^8.1.0",
    "ramda": "^0.26.1",
    "superagent": "^6.1.0",
    "tslib": "^1.14.1",
    "yaml": "^1.10.2"
  }
}
